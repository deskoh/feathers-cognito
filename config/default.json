{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "tests": {
    "environmentsAllowingSeedData": [
      "test"
    ],
    "local": {
      "password": "password"
    },
    "client": {
      "port": 3030,
      "ioOptions": {
        "transports": [
          "websocket"
        ],
        "forceNew": true,
        "reconnection": false,
        "extraHeaders": {}
      },
      "primusOptions": {
        "transformer": "ws"
      },
      "restOptions": {
        "url": "http://localhost:3030"
      },
      "overriddenAuth": {}
    }
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "user",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    },
    "local": {
      "entity": "user",
      "usernameField": "_id",
      "passwordField": "password"
    },
    "cognito": {
      "clientID": "COGNITO_CLIENTID",
      "clientSecret": "COGNITO_CLIENT_SECRET",
      "clientDomain": "COGNITO_DOMAIN",
      "callbackURL": "COGNITO_CALLBACK_URL",
      "successRedirect": "COGNITO_SUCCESS_REDIRECT",
      "region": "COGNITO_REGION"
    }
  },
  "mongodb": "mongodb://localhost:27017/feathers-cognito",
  "sync": {
    "uri": "redis://localhost:6379"
  },
  "arcgisServer": "http://arcgisserver.web.saf.sg/arcgis/rest/services/NDP19/"
}
